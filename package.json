{
  "name": "jstdd",
  "version": "1.0.0",
  "description": "Nossa biblioteca do curso de JS com TDD na pr√°tica.",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run test:coverage",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": ["text", "html"],
    "exclude": ["tests/**"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateusduraes/jstdd-couse.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library",
    "course"
  ],
  "author": "Mateus Duraes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mateusduraes/jstdd-couse/issues"
  },
  "homepage": "https://github.com/mateusduraes/jstdd-couse#readme",
  "devDependencies": {
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "husky": "^0.14.3",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0"
  }
}
